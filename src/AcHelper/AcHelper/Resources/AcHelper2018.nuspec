<?xml version="1.0" encoding="utf-8" ?>
<package >

  <metadata>
    <id>AcHelper.2018</id>
    <version>$version$</version>
    <title>AcHelper</title>
    <authors>Sander van Bueren (BuerTech)</authors>
    <owners>BuerTech</owners>
    <description>Assembly which simplifies the use of some AutoCAD functions for the developer.</description>

    <projectUrl>https://github.com/BikerMouse/AcHelper</projectUrl>
    <requireLicenseAcceptance>false</requireLicenseAcceptance>

    <releaseNotes>First release as a version separated assembly.</releaseNotes>
    <copyright>Copyright © BuerTech 2017</copyright>
    <tags>AutoCAD BuerTech</tags>

    <dependencies>
      <dependency id="BuerTech.Logger" version="2.0.4.3" />
      <dependency id="AutoCAD.Net" version="20.0.0" />
      <dependency id="AutoCAD.Net.Core" version="20.0.0" />
      <dependency id="AutoCAD.Net.Model" version="20.0.0" />
    </dependencies>
  </metadata>

  <files>
    <file src="Release\2018\**" target="lib\net46"/>
    <file src="Release\2018\**" target="build\x86"/>

    <file src="x64\Release\2018\**" target="build\x64"/>

    <file src="AcHelper.props" target="build\net46"/>
  </files>
</package>